{
	"name": "My First Extension",
	"version": "1.0",
	"description": "The first extension that I made.",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Code",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"activeTab"
	],
	"background": {
    	"scripts": ["background.js"],
    	"persistent": false
  	}
}